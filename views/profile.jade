extends default
block content
  h3 Profile Page
  br
  h4 Your Keywords
  table.table.table-striped.table-bordered.table-hover.table-condensed
    thead
      tr
        th Alias
        th Uses
        th Description
    tbody
      each keyword in keywords
        tr
          td #{keyword.alias}
          td #{keyword.uses}
          td #{keyword.descrip}



